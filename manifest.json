{
  "manifest_version": 3,
  "name": "golinks",
  "homepage_url": "https://github.com/jkulton/golinks",
  "version": "0.3.0",
  "description": "golinks in your browser",
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "background": {
    "service_worker": "main.js",
    "type": "module"
  },
  "permissions": [
    "declarativeNetRequest",
    "storage"
  ],
  "host_permissions": [
    "*://go/*"
  ],
  "options_page": "./pages/index.html"
}